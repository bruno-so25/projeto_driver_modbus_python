    {
        "id": "1410ddaf3b37b2e4",
        "type": "function",
        "z": "a1347b0421f1451a",
        "name": "Prepara Dados FORM1",
        "func": "const flat = flow.get(\"SETTINGS\") || {};\n\nmsg.payload = {\n    \"MODBUS.host\": flat[\"MODBUS.host\"] || \"\",\n    \"MODBUS.port\": flat[\"MODBUS.port\"] || \"\",\n    \"MODBUS.unit_id\": flat[\"MODBUS.unit_id\"] || \"\",\n\n    \"DEVICE.vendor_name\": flat[\"DEVICE.vendor_name\"] || \"\",\n    \"DEVICE.product_code\": flat[\"DEVICE.product_code\"] || \"\",\n    \"DEVICE.vendor_url\": flat[\"DEVICE.vendor_url\"] || \"\",\n    \"DEVICE.product_name\": flat[\"DEVICE.product_name\"] || \"\",\n    \"DEVICE.revision\": flat[\"DEVICE.revision\"] || \"\",\n\n    \"MEMORY.default_value\": flat[\"MEMORY.default_value\"] || \"\",\n    \"MEMORY.hr_count\": flat[\"MEMORY.hr_count\"] || \"\",\n    \"MEMORY.coil_count\": flat[\"MEMORY.coil_count\"] || \"\",\n    \"MEMORY.di_count\": flat[\"MEMORY.di_count\"] || \"\",\n    \"MEMORY.ir_count\": flat[\"MEMORY.ir_count\"] || \"\",\n\n    \"WATCHDOG.enabled\": flat[\"WATCHDOG.enabled\"] || \"\",\n    \"WATCHDOG.max_retries\": flat[\"WATCHDOG.max_retries\"] || \"\",\n    \"WATCHDOG.interval_seconds\": flat[\"WATCHDOG.interval_seconds\"] || \"\"\n};\n\nreturn msg;",
